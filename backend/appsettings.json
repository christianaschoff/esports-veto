{
  "VetoDatabase": {
    "ConnectionString": "<VetoDatabaseConnectionString>",
    "DatabaseName": "VetoSystem",
    "VetoCollectionName": "VetoConfig",
    "VetoResultCollectionName": "VetoResult"
  },
  "Token": {
    "key": "<YourSecretKey>"
  },
  "RateLimiter": {
    "TokenLimit": 15,
    "QueueLimit": 30,
    "ReplenishmentPeriod": 15,
    "TokensPerPeriod": 200,
    "AutoReplenishment": true
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "OTLP_ENDPOINT_URL": null,
  "ZIPKIN_ENDPOINT_URL": "http://localhost:9411/api/v2/spans",
  "UseOpenTelemetry": true
}
