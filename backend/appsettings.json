{
  "VetoDatabase": {
    "ConnectionString": "<VetoDatabaseConnectionString>",
    "DatabaseName": "VetoSystem",
    "VetoCollectionName": "VetoConfig",
    "VetoResultCollectionName": "VetoResult"
  },
  "Token" : {
    "key": "<YourSecreKey>"
  },
  "RateLimiter": {
    "TokenLimit": 15,
    "QueueLimit": 30,
    "ReplenishmentPeriod": 15,
    "TokensPerPeriod": 200,
    "AutoReplenishment": true
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  }
}
